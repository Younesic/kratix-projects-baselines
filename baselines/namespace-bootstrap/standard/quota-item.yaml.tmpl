- apiVersion: v1
  kind: ResourceQuota
  metadata:
    name: ${NS_NAME}-quota
    namespace: ${NS_NAME}
    labels:
      baseline.kratix.io/version: ${BASELINE_VERSION}
      baseline.kratix.io/size: ${BASELINE_SIZE}
    annotations:
      baseline.kratix.io/version: ${BASELINE_VERSION}
      baseline.kratix.io/size: ${BASELINE_SIZE}
  spec:
    hard:
      requests.cpu: "1"
      limits.cpu: "2"
      requests.memory: 2Gi
      limits.memory: 4Gi
      pods: "40"
